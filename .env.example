# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=malaysia_data
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_secure_password

# Airflow metadata DB (internal)
AIRFLOW_DB_USER=airflow_user
AIRFLOW_DB_PASSWORD=airflow_password
AIRFLOW_DB_NAME=airflow

# API Configuration (data.gov.my - no authentication required for public data)
API_BASE_URL=https://api.data.gov.my/data-catalogue

# Optional: Additional API configurations
# API_RATE_LIMIT=1000  # requests per hour
# API_RETRY_MAX=3
# API_RETRY_DELAY=1.0
